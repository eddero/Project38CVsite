@model IEnumerable<Data.Models.Message>

@{
    ViewBag.Title = "GetMessage";
}

<h2>GetMessage</h2>

<table class="table">
    

    
@foreach (var item in Model) {
    <tr>

        <td>
            <span>From Name/ User:</span>
            @Html.DisplayFor(modelItem => item.FromUser.FirstName)
        </td>

        <td>
            <span>From Name/ User:</span>
            @Html.DisplayFor(modelItem => item.FromName)
        </td>
        <td>
            <span>Content:</span>
            @Html.DisplayFor(modelItem => item.Content)
        </td>

        <td>
            <span>IsRead:</span>
            @Html.DisplayFor(modelItem => item.IsRead)
        </td>

        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
        </td>


    </tr>
}

</table>
